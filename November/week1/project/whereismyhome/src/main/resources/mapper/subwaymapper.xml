<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.ssafy.subway.mapper.SubwayMapper">


	<resultMap type="Subway" id="subs">
		<result column="kakaoBunji" property="kakaoBunji"/>
	</resultMap>
	
	<select id="getSubway" parameterType="String" resultType="String">
		select kakaoBunji from subway
		where substr(kakaoBunji,1,2) = #{sido}
	</select>
	

	
</mapper>

